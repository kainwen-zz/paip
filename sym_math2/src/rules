x + 0  = x
0 + x  = x
x + x  = 2 * x
x - 0  = x
0 - x  = ~ x
x - x  = 0
~ ~ x  = x
x * 1  = x
1 * x  = x
x * 0  = 0
0 * x  = 0
x * x  = x ^ 2
x / 0  = undefined
0 / x  = 0
x / 1  = x
x / x  = 1
0 ^ 0  = undefined
x ^ 0  = 1
0 ^ x  = 0
1 ^ x  = 1
x ^ 1  = x
x ^ ~1 = 1 / x
x * (y / x) = y
(y / x) * x = y
(y * x) / x = y
(x * y) / x = y
x + ~ x = 0
(~ x) + x = 0
x + y - x = y
x ^ n / x = x ^ (n-1) with N > 0.
s * n = n * s with is_number(N) and not is_number(S).
n * (m * x) = (n * m) * x with is_number(N) and is_number(M).
x * (n * y) = n * (x * y) with is_number(N).
(n * x) * y = n * (x * y) with is_number(N).
n + s = s + n with is_number(N) and not is_number(S).
(x + m) + n = x + (m + n) with is_number(N) and is_number(M).
x + (y + n) = (x + y) + n with is_number(N).
(x + n) + y = (x + y) + n with is_number(N).
y * (1 / x) = y / x
~ (n * x) = (~n) * x with is_number(N).
log{1} = 0
log{0} = undefined
log{exp{1}} = 1
sin{0} = 0
sin{pi} = 0
cos{0}  = 1
cos{pi} = ~1
sin{pi/2} = 1
cos{pi/2} = 0
log{exp{x}} = x
exp{log{x}} = x
(x ^ y) * (x ^ z) = x ^ (y + z)
(x ^ y) / (x ^ z) = x ^ (y - z)
log{x} + log{y} = log{x * y}
log{x} - log{y} = log{x / y}
(sin{x}) ^ 2 + (cos{x}) ^ 2 = 1
diff{x, x} = 1
diff{u + v, x} = diff{u, x} + diff{v, x}
diff{u - v, x} = diff{u, x} - diff{v, x}
diff{~u, x} = ~diff{u,x}
diff{u*v, x} = u*diff{v, x} + v*diff{u,x}
diff{u/v, x} = (diff{u, x}*v-u*diff{v, x}) / v^2
diff{u^n, x} = n*u^(n-1)*diff{u,x} with is_number(N).
diff{u^v, x} = v*u^(v-1)*diff{u, x} + u*v*log_{u}*diff{v,x}
diff{log{u}, x} = diff{u, x} / u
diff{sin{u}, x} = cos{u} * diff{u, x}
diff{cos{u}, x} = ~sin{u} * diff{u, x}
diff{exp{u}, x} = exp{u} * diff{u, x}
diff{u, n*x} = diff{u, x} / n with is_number(N).
int{u+v, x} = int{u, x} + int{v, x}
int{u-v, x} = int{u, x} - int{v, x}
int{n*v, x} = n*int{v, x} with is_number(N).
int{log{x}, x} = x*log{x} - x
int{exp{x}, x} = exp{x}
int{sin{x}, x} = ~cos{x}
int{cos{x}, x} = sin{x}
int{1/x, x} = log{x}
int{u, n*x} = n*int{u, x} with is_number(N).
int{u, n*x+m} = n*int{u, x} with is_number(N) and is_number(M).
diff{u, x} = 0
